<?xml version="1.0" encoding="UTF-8"?>

<zk>
    <!-- [Optional]
    Uncomment if you want to defines the application's name

    <preference>
        <name>org.zkoss.zk.ui.WebApp.name</name>
        <value>cred-mngr-poc</value>
    </preference>
    -->
    <library-property>
        <name>org.zkoss.zul.image.preload</name>
        <value>true</value>
    </library-property>
    <library-property>
        <name>org.zkoss.zul.Button.mold</name>
        <value>bs</value>
    </library-property>
    <library-property>
        <name>org.zkoss.zul.Menupopup.mold</name>
        <value>bs</value>
    </library-property>
    <!--library-property>
        <name>org.zkoss.zul.Panel.mold</name>
        <value>bs</value>
    </library-property-->
    <library-property>
        <name>org.zkoss.web.preferred.locale</name>
        <value>en</value>
    </library-property>
    <library-property>
        <name>org.zkoss.util.label.web.charset</name>
        <value>ISO-8859-1</value>
    </library-property>
    <system-config>
        <label-location>/WEB-INF/labels/explain_area.properties</label-location>
        <label-location>/WEB-INF/labels/user.properties</label-location>
        <label-location>/WEB-INF/labels/general.properties</label-location>
    </system-config>
    <client-config>
        <tooltip-delay>70</tooltip-delay>
    </client-config>
</zk>
